{
  "name": "@wayke-se/components-react",
  "version": "4.0.1",
  "description": "",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "test": "npm run lint",
    "lint": "eslint --ext .tsx,.ts 'src' 'example'",
    "lint:fix": "eslint --ext .tsx,.ts 'src' 'example' --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "gen": "graphql-codegen --config ./codegen.yml",
    "generate-libphonenumber-metadata": "libphonenumber-metadata-generator src/utils/metadata.min.json --countries SE,NO --extended",
    "prepare": "husky install",
    "start": "node ./esbuild/start.js",
    "build": "npm run clean && node ./esbuild/lib.js",
    "clean": "rimraf build && rimraf dist && rimraf dist-cdn",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayke/components-react.git"
  },
  "author": "Ourstudio",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@types/googlemaps": "^3.43.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-slick": "^0.23.7",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@wayke-se/ecom-web": "^1.5.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.27.0",
    "husky": "^8.0.1",
    "libphonenumber-metadata-generator": "^1.0.1",
    "npm-dts": "^1.3.12",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.2",
    "rimraf": "^5.0.5",
    "styled-components": "^5.3.11",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@wayke-se/pannellum-react": "^1.0.0",
    "date-fns": "^2.25.0",
    "graphql": "^16.0.1",
    "i18next": "^23.6.0",
    "libphonenumber-js": "^1.9.42",
    "marked": "^10.0.0",
    "react-compound-slider": "^3.3.1",
    "react-i18next": "^13.3.1",
    "react-slick": "^0.29.0",
    "scroll-into-view-if-needed": "^3.1.0"
  }
}
