    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.1",
    "dotenv": "^8.2.0",
