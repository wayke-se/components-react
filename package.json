{
  "name": "@wayke-se/components-react",
  "version": "1.0.0-alpha.1",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "pika build",
    "prebuild": "npm run clean",
    "lint": "eslint --ext .tsx,.ts 'src'",
    "clean": "rimraf ./dist ./pkg ./coverage",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "npm run prettify && npm run type-check && npm run lint && npm run test",
    "generate": "apollo codegen:generate --excludes=node_modules/* --includes=./src/queries/**/* --localSchemaFile=schema.graphql --target typescript --tagName gql --outputFlat src/@types/gql"
  },
  "author": "Peter Johansson",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@apollo/react-common": "^3.1.4",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-testing": "^3.1.4",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0"
  },
  "workspaces": {
    "nohoist": [
      "react"
    ]
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
