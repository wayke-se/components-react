    "react-slick": "^0.26.1",
    "slick-carousel": "^1.8.1"
  },
  "workspaces": {
    "nohoist": [
      "react"
    ]
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "args": [
            "--target",
            "es2019"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
