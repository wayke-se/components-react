    "@wayke-se/ecom-react": "^3.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
